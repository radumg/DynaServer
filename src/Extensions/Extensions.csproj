  <!--Using AfterBuild Events to copy the extension to the Dynamo packages folder-->
  <Target Name="AfterBuild">
    <Message Importance="High" Text="++++++++++++++++++++++++++++++++++++++" />
    <Message Importance="High" Text="Dynamo extension post-build" />
    <!--Defining folders to copy-->
    <PropertyGroup>
      <PackageFolder>$(SolutionDir)..\package</PackageFolder>
    </PropertyGroup>
    <ItemGroup>
      <SourceDlls Include="$(TargetDir)*.dll" />
      <SourcePdbs Include="$(TargetDir)*.pdb" />
      <SourcePkg Include="$(ProjectDir)pkg.json" />
      <SourceExtension Include="$(ProjectDir)Resources\DynamoServer_ExtensionDefinition.xml" />
      <SourceViewExtension Include="$(ProjectDir)Resources\DynamoServer_ViewExtensionDefinition.xml" />
      <SourceHtml Include="$(TargetDir)Views\**\*.*" />
      <PackageFiles Include="$(PackageFolder).\**\*.*"/>
    </ItemGroup>
    <!--Copying to Build Folder-->
    <Message Importance="High" Text="++++++++++++++++++++++++++++++++++++++ Assembling to package folder" />
    <Message Importance="High" Text="SourceDlls = @(SourceDlls)" />
    <Message Importance="High" Text="SourcePdbs = @(SourcePdbs)" />
    <Message Importance="High" Text="SourcePkg = @(SourcePkg)" />
    <Message Importance="High" Text="SourceExtension = @(SourceExtension)" />
    <Message Importance="High" Text="SourceViewExtension = @(SourceViewExtension)" />
    <Message Importance="High" Text="SourceHtml = @(SourceHtml)" />
    <Message Importance="High" Text="PackageFolder = $(PackageFolder)" />

    <Copy SourceFiles="@(SourceDlls)" DestinationFolder="$(PackageFolder)\bin" SkipUnchangedFiles="false"/>
    <Copy SourceFiles="@(SourcePdbs)" DestinationFolder="$(PackageFolder)\bin" SkipUnchangedFiles="false"/>
    <Copy SourceFiles="@(SourcePkg)" DestinationFolder="$(PackageFolder)" SkipUnchangedFiles="false"/>
    <Copy SourceFiles="@(SourceExtension)" DestinationFolder="$(PackageFolder)\extra" SkipUnchangedFiles="false"/>
    <Copy SourceFiles="@(SourceViewExtension)" DestinationFolder="$(PackageFolder)\extra" SkipUnchangedFiles="false" />
    <Copy SourceFiles="@(SourceHtml)" DestinationFolder="$(PackageFolder)\extra" SkipUnchangedFiles="false"/>
    <PropertyGroup>
      <!--Copy to Dynamo sandbox for testing -->
      <DeployFolder>$(AppData)\Dynamo\Dynamo Core\2.0\packages\$(SolutionName)</DeployFolder>
    </PropertyGroup>
    <Message Importance="High" Text="++++++++++++++++++++++++++++++++++++++" />
    <Message Importance="High" Text="Deployed to $(DeployFolder)" />
    
    <MakeDir Directories="$(DeployFolder)"/>
    <Copy SourceFiles="@(PackageFiles)" DestinationFolder="$(DeployFolder)\%(RecursiveDir)" SkipUnchangedFiles="false" />
  </Target>
